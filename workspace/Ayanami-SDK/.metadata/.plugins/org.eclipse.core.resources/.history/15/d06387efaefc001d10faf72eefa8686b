#ifndef AYA_DRIVER_I2C_H_
#define AYA_DRIVER_I2C_H_
#include "ayanami_common/aya_common_headfile.h"

typedef enum
{
    I2C_0=0,
    I2C_1,
    I2C_2,
    I2C_3
}i2c_index_enum;



void i2c_init(i2c_index_enum i2c_n);
void i2c_write(i2c_index_enum i2c_n,uint8_t *data,uint8_t len);
void i2c_read(i2c_index_enum i2c_n,uint8_t* data, uint8_t len)

#endif /* AYANAMI_DRIVER_AYA_DRIVER_I2C_H_ */
